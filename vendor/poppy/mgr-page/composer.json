{
	"name": "poppy/mgr-page",
	"description": "An admin management page module.",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Mark Zhao",
			"email": "zhaody901@126.com"
		}
	],
	"require": {
		"php": ">=7.2.0",
		"poppy/system": "3.0.x-dev",
		"mews/captcha": "3.*"
	},
	"autoload": {
		"psr-4": {
			"Poppy\\MgrPage\\": "src/",
			"Poppy\\MgrPage\\Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Poppy\\MgrPage\\ServiceProvider"
			],
			"aliases": {
				"PyForm": "Poppy\\MgrPage\\Facade\\FormFacade"
			}
		}
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
