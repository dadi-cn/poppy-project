{
	"name" : "laravel/laravel",
	"description" : "The Laravel Framework.",
	"keywords" : [
		"framework",
		"laravel"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php" : "^7.0",
		"ext-json" : "*",
		"ext-simplexml" : "*",
		"ext-openssl" : "*",
		"ext-pdo" : "*",
		"ext-gd" : "*",
		"ext-curl" : "*",
		"ext-mbstring" : "*",
		"ext-mcrypt" : "*",
		"ext-bcmath" : "*",
		"laravel/framework" : "5.5.*",
		"intervention/image" : "^2.4",
		"laravel/tinker" : "~1.0",
		"laravelcollective/html" : "5.5.*",
		"mews/purifier" : "~2.0",
		"mews/captcha" : "~2.0",
		"nesbot/carbon" : "~1.21",
		"predis/predis" : "~1.1",
		"doctrine/dbal" : "2.5.*",
		"guzzlehttp/guzzle" : "6.*",
		"php-curl-class/php-curl-class" : "~8.5",
		"tymon/jwt-auth" : "1.0.*@dev",
		"tucker-eric/eloquentfilter" : "^1.2",
		"socialiteproviders/manager" : "~3.3",
		"sabberworm/php-css-parser" : "*",
		"overtrue/phplint" : "^1.0",
		"poppy/framework" : "dev-master",
		"hieu-le/active" : "^3.5"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6.0",
		"itsgoingd/clockwork" : "~3.1",
		"barryvdh/laravel-ide-helper" : "~2.4",
		"fzaninotto/faker" : "~1.7",
		"phpspec/phpspec" : "^4.3"
	},
	"autoload" : {
		"classmap" : [
		],
		"psr-4" : {
			"System\\" : "modules/system/src/",
			"System\\Tests\\" : "modules/system/tests/"
		}
	},
	"autoload-dev" : {
		"classmap" : [
		],
		"psr-4" : {
		}
	},
	"extra" : {
		"laravel" : {
			"dont-discover" : [
				"laravelcollective/html"
			]
		}
	},
	"scripts" : {
		"post-install-cmd" : [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate"
		],
		"pre-update-cmd" : [
		],
		"post-update-cmd" : [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan ide-helper:meta"
		],
		"post-root-package-install" : [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd" : [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config" : {
		"preferred-install" : "dist"
	},
	"repositories" : [
		{
			"type" : "composer",
			"url" : "https://packagist.laravel-china.org"
		},
		{
			"packagist" : false
		}
	],
	"prefer-stable" : true
}
