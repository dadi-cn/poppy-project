{
	"name": "poppy/demo",
	"description": "The Poppy Demo Project",
	"keywords": [
		"framework",
		"laravel",
		"poppy"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.2",
		"ext-dom": "*",
		"ext-json": "*",
		"ext-simplexml": "*",
		"ext-openssl": "*",
		"ext-pdo": "*",
		"ext-gd": "*",
		"ext-curl": "*",
		"ext-mbstring": "*",
		"ext-bcmath": "*",
		"laravel/framework": "6.*",
		"intervention/image": "2.*",
		"laravel/tinker": "~2",
		"laravelcollective/html": "6.*",
		"predis/predis": "~1.1",
		"poppy/framework": "3.0.x-dev",
		"poppy/system": "3.0.x-dev",
		"poppy/core": "3.0.x-dev",
		"poppy/mgr-page": "3.0.x-dev"
	},
	"require-dev": {
		"itsgoingd/clockwork": "~5.0",
		"barryvdh/laravel-ide-helper": "~2.6",
		"poppy/faker": "3.0.x-dev",
		"doctrine/dbal": "^2.5"
	},
	"autoload": {
		"classmap": [
		]
	},
	"autoload-dev": {
		"classmap": [
		],
		"psr-4": {
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packagist.iliexiang.com"
		}
	],
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate"
		],
		"pre-update-cmd": [
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan ide-helper:generate",
			"php artisan ide-helper:meta"
		],
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
